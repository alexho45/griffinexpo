<%- headers = ['Attendee Name', 'Company Name', 'Checked', 'Seminars'] -%>
<%= CSV.generate_line(headers).strip %>
<%- attendees.each do |attendee| -%>
<%= CSV.generate_line(
  [
    attendee.full_name,
    attendee.company.try(:full_name),
    checked_attendees.include?(attendee),
    attendee.checked_seminars
  ]).html_safe
-%>
<%- end -%>
